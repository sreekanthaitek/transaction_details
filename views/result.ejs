<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Results</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"></script>
    <link rel="icon" href="logo.jpeg" style="border-radius: 25px;">
</head>
<body>
    <% data.forEach(entry => { %>
        <div id="<%= entry._id %>">
            <div class="row">
                <div class="col">
                    <div class="card mx-3 my-3 p-2">
                        <table class="table border-dark table-bordered">
                            <tr>
                                <td><b><i>Date: </i></b></td>
                                <td><%= entry.date %></td>
                                <td></td>
                                <td></td>
                            </tr>
                            <tr>
                                <td><b><i>Transaction Number: </i></b></td>
                                <td><%= entry.tran %></td>
                                <td></td>
                                <td></td>
                            </tr>
                            <tr>
                                <td><b><i>Voucher: </i></b></td>
                                <td><%= entry.voucher %></td>
                                <td></td>
                                <td></td>
                            </tr>
                            <tr>
                                <td><b><i>Curency: </i></b></td>
                                <td><%= entry.sample_1%></td>
                                <td></td>
                                <td></td>
                            </tr>
                            <tr>
                                <td><b><i>Account Name </i></b></td>
                                <td><b><i>Particulars</i></b></td>
                                <td><b><i>Debit</i></b></td>
                                <td><b><i>Credit</i></b></td>
                            </tr>
                            <tr>
                                <td><%= entry.sample_2 %></td>
                                <td><%= entry.name_1 %></td>
                                <td><%= entry.debit_1d %></td>
                                <td><%= entry.credit_1d %></td>
                            </tr>
                            <tr>
                                <td></td>
                                <td><%= entry.name_2 %></td>
                                <td><%= entry.debit_2d %></td>
                                <td><%= entry.credit_2d %></td>
                            </tr>
                            <tr>
                                <td></td>
                                <td><%= entry.name_3 %></td>
                                <td><%= entry.debit_3d %></td>
                                <td><%= entry.credit_3d %></td>
                            </tr>
                            <tr>
                                <td></td>
                                <td><%= entry.name_4 %></td>
                                <td><%= entry.debit_4d %></td>
                                <td><%= entry.credit_4d %></td>
                            </tr>
                            <tr>
                                <td></td>
                                <td><%= entry.name_5 %></td>
                                <td><%= entry.debit_5d %></td>
                                <td><%= entry.credit_5d %></td>
                            </tr>
                            <tr>
                                <td></td>
                                <td><%= entry.name_6 %></td>
                                <td><%= entry.debit_6d %></td>
                                <td><%= entry.credit_6d %></td>
                            </tr>
                            <tr>
                                <td></td>
                                <td><%= entry.name_7 %></td>
                                <td><%= entry.debit_7d %></td>
                                <td><%= entry.credit_7d %></td>
                            </tr>
                            <tr>
                                <td class="bg-warning"><b><%= entry.comment %></b></td>
                                <td class="bg-warning"></td>
                                <td class="bg-warning"><%= entry.totalDebit %></td>
                                <td class="bg-warning"><%= entry.totalCredit %></td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    <% }) %>
</body>
</html>
